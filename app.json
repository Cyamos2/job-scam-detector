{
  "expo": {
    "name": "Scamicide",
    "slug": "scamicide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/scamicide-logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "assets/scamicide-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.scamicide",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow taking photos to analyze screenshots.",
        "NSPhotoLibraryUsageDescription": "Allow selecting screenshots from your photo library to analyze.",
        "NSPhotoLibraryAddUsageDescription": "Save analyzed screenshots to your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/scamicide-logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ],
      "package": "com.anonymous.scamicide",
      "versionCode": 1
    },
    "plugins": [
      "sentry-expo"
    ],
    "extra": {
      "eas": {
        "projectId": "YOUR_EXPO_PROJECT_ID"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/YOUR_EXPO_PROJECT_ID",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes"
  }
}

